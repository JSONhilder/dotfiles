[iface]
command=./scripts/iface
LABEL=Connected
#IFACE=wlan0
#ADDRESS_FAMILY=inet6?
color=#99ad6a
interval=10
# set this to 1 to display the name of the connected WIFI interface instead of the IP address.
display_wifi_name=0

[volume-pipewire]
command=./scripts/volume-pipewire -f
interval=once
signal=1
SHORT_FORMAT="${SYMB} ${VOL}%"

